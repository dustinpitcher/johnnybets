{
  "name": "johnnybets-maintenance-worker",
  "version": "1.0.0",
  "private": true,
  "description": "Cloudflare Worker for JohnnyBets maintenance page",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:staging": "wrangler deploy --env staging",
    "tail": "wrangler tail",
    "enable": "wrangler secret put MAINTENANCE_MODE <<< 'true'",
    "disable": "wrangler secret put MAINTENANCE_MODE <<< 'false'"
  },
  "devDependencies": {
    "wrangler": "^3.0.0"
  }
}
